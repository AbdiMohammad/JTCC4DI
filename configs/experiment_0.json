{
    "PSNR": 10,
    "dataset": "cifar10",
    "model": "resnet20",
    "cifar_pretrained_weights_path": "cifar_pretrained/resnet20-12fca82f.th",
    "batch_size": 256,
    "n_epochs": 300,
    "pretrained_weights_path": null,
    "save_weights_after_train_path": "output/experiment_2/weights_after_train.pth",
    "codebook_lr": 0.001,
    "non_codebook_lr": 0.0001,
    "output_folder": "output/experiment_2",
    "codebooks":[
        {
            "layer": "layer1[2]",
            "hidden_dim": 16,
            "codebook_size": 16,
            "beta": "LinearCoefficient(1e-3, 1e-4)",
            "prune_epochs": [50 , 100],
            "prune_ns": [4, 4]
        }
    ],

    "prune_channels": [
        {
            "layer": "layer1[0].conv1",
            "gamma": "LinearCoefficient(1e-4, 1e-5)",
            "prune_epochs": [50, 100],
            "prune_ns": [1, 2]
        },
        {
            "layer": "layer1[0].conv2",
            "gamma": "LinearCoefficient(1e-4, 1e-5)",
            "prune_epochs": [50, 100],
            "prune_ns": [1, 2]
        },
        {
            "layer": "layer1[1].conv1",
            "gamma": "LinearCoefficient(2.0 * 1e-4, 2.0 * 1e-5)",
            "prune_epochs": [50, 100],
            "prune_ns": [2, 4]
        },
        {
            "layer": "layer1[1].conv2",
            "gamma": "LinearCoefficient(2.0 * 1e-4, 2.0 * 1e-5)",
            "prune_epochs": [50, 100],
            "prune_ns": [2, 4]
        },
        {
            "layer": "layer1[2].conv1",
            "gamma": "LinearCoefficient(3.0 * 1e-4, 3.0 * 1e-5)",
            "prune_epochs": [50, 100],
            "prune_ns": [3, 6]
        },
        {
            "layer": "layer1[2].conv2",
            "gamma": "LinearCoefficient(3.0 * 1e-4, 3.0 * 1e-5)",
            "prune_epochs": [50, 100],
            "prune_ns": [3, 6]
        }
    ]
}