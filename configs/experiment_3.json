{
    "PSNR": 4,
    "dataset": "cifar10",
    "model": "resnet1202",
    "reference_pretrained_weights_path": "resource/cifar_pretrained/resnet1202-f3b1deed.th",
    "batch_size": 1024,
    "n_epochs": 500,
    "pretrained_weights_path": null,
    "save_weights_after_train_path": true,
    "codebook_lr": 0.001,
    "non_codebook_lr": 0.0001,
    "output_folder": "output/experiment_3",
    "codebooks":[
        {
            "layer": "layer1[199]",
            "hidden_dim": 16,
            "codebook_size": 16,
            "beta": "LinearCoefficient(1e-4, 1e-3)",
            "prune_epochs": [80, 90, 100, 110, 120],
            "prune_value": 50,
            "prune_value_type": "percentage"
        }
    ],

    "funnel": {
        "layer": "layer1[199].conv2",
        "gamma": 1e-5,
        "prune_epochs": [80, 90, 100, 110, 120],
        "prune_value": 50,
        "prune_value_type": "percentage"
    }
}